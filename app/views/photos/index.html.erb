<div class="container">
  <div class="row">
       <div class="col s12 m7">
         <div class="card photo-of-the-day">
           <div class="card-image">
             <%= image_tag @photo_of_the_day.photo %>
             <span class="card-title"><%= @photo_of_the_day.title %></span>
           </div>
           <div class="card-content">
             <p><%= @photo_of_the_day.caption %></p>
           </div>
         </div>
       </div>
     </div>
</div>
