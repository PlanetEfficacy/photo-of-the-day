<!DOCTYPE html>
<html>
  <head>
    <title>PhotoOfTheDay</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
    <%= stylesheet_link_tag "https://fonts.googleapis.com/icon?family=Material+Icons" %>
    <%= stylesheet_link_tag "https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.8/css/materialize.min.css" %>
    <%= javascript_include_tag "https://www.google.com/jsapi", "chartkick" %>
  </head>

  <body>
    <%= render 'shared/header/navbar' %>
    <div class="small-space">

    </div>
    <div class="container">
      <div class="indigo">
        <i class="material-icons white-text" style="font-size:500px">g_translate</i>
      </div>
    </div>

    <%= yield %>
  </body>

  <%= javascript_include_tag 'application' %>
</html>
